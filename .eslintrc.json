{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc" : 0,
    "indent": [2, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
    "translate_tabs_to_spaces": true
  },
  "plugins": [
    "json"
  ]
}
